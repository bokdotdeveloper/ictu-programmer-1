{
    "version": 2,
    "builds": [
      {
        "src": "backend/src/index.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["backend/src/**/*.ts"]
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/src/index.ts"
      }
    ],
    "env": {
      "PORT": "5000",
      "COOKIE_SECRET": "@cookie_secret",
      "DATABASE_URL": "@database_url"
    }
  }
  